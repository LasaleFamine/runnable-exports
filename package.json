{
  "name": "runnable-exports",
  "version": "0.1.1",
  "description": "Call exports module from command line.",
  "main": "src/index.js",
  "engines": {
    "node": ">=6.0.0"
  },
  "contributors": [
    "emurphy-9"
  ],
  "scripts": {
    "lint": "xo",
    "pretest": "yarn lint",
    "test": "yarn test-only",
    "test-only": "nyc ava",
    "report": "nyc report --reporter=html",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "ava": {
    "files": [
      "tests/**/*.spec.js",
      "!tests/testfiles/*.js"
    ]
  },
  "xo": {
    "semicolon": true,
    "rules": {
      "comma-dangle": 0,
      "no-else-return": 0,
      "unicorn/filename-case": 0
    },
    "envs": [
      "node"
    ],
    "ignores": [
      "tests/**/*.js"
    ]
  },
  "keywords": [
    "runnable",
    "runnable-exports",
    "exports",
    "module",
    "export",
    "run",
    "function",
    "func",
    "exec",
    "execute"
  ],
  "repository": "https://github.com/lasalefamine/runnable-exports",
  "author": {
    "name": "LasaleFamine",
    "email": "info@godev.space"
  },
  "license": "MIT",
  "devDependencies": {
    "ava": "0.25.0",
    "coveralls": "3.0.2",
    "nyc": "13.0.1",
    "standard-version": "4.4.0",
    "xo": "0.23.0"
  },
  "dependencies": {
    "auto-bind": "1.2.1",
    "yargs": "12.0.2"
  }
}
